{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently --kill-others \"npm run watch:css\" \"remix run\"",
    "start": "remix-serve build",
    "watch:css": "postcss styles --base styles --dir ./app/styles -w",
    "build:css": "postcss styles --base styles --dir ./app/styles --env production"
  },
  "dependencies": {
    "@remix-run/node": "^0.17.5",
    "@remix-run/react": "^0.17.5",
    "@remix-run/serve": "^0.17.5",
    "prettier": "^2.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.17.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.17.5",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
